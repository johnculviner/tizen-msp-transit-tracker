<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:tizen="http://tizen.org/ns/widgets" id="http://yourdomain/<%= projectName %>" version="1.0.0" viewmodes="maximized">
    <access origin="*" subdomains="true"/>
    <tizen:application id="<%= projectId %>.<%= projectName %>" package="<%= projectId %>" required_version="3.0"/>
    <content src="index.html"/>
    <feature name="http://tizen.org/feature/screen.size.all"/>
    <icon src="icon.png"/>
    <name><%= displayName %></name>
    <% _.forEach(privileges, function(privilege) { %><tizen:privilege name="http://tizen.org/privilege/<%- privilege %>"/><% }); %>
    <% _.forEach(features, function(feature) { %><tizen:privilege name="http://tizen.org/feature/<%- feature %>"/><% }); %>
    <tizen:profile name="wearable"/>
</widget>
